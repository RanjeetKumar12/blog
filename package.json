{
  "name": "urara",
  "version": "0.0.1",
  "scripts": {
    "clean": "node urara.js clean",
    "dev:watch": "node urara.js watch",
    "dev:kit": "export NODE_OPTIONS=--max_old_space_size=6144 && svelte-kit dev",
    "dev": "npm-run-all -p -r dev:watch \"dev:kit -- {@} \" --",
    "prebuild": "node urara.js build",
    "build": "export NODE_OPTIONS=--max_old_space_size=6144 && svelte-kit build",
    "postbuild": "node urara.js clean",
    "preview": "export NODE_OPTIONS=--max_old_space_size=6144 && svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. ."
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.23",
    "@sveltejs/adapter-static": "^1.0.0-next.19",
    "@sveltejs/adapter-vercel": "^1.0.0-next.31",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.2",
    "daisyui": "^1.14.1",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-svelte3": "^3.2.0",
    "mdsvex": "^0.9.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "rehype-external-links": "^1.0.0",
    "rehype-slug": "^5.0.0",
    "remark": "^14.0.1",
    "remark-abbr": "^1.4.1",
    "remark-spoiler": "^1.0.6",
    "shiki": "^0.9.6",
    "svelte": "^3.34.0",
    "svelte-check": "^2.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "vite": "^2.6.13",
    "vite-plugin-windicss": "^1.2.4"
  },
  "type": "module"
}
