{
	"name": "urara",
	"version": "0.0.1",
	"scripts": {
		"clean": "node urara.js clean",
		"watch": "node urara.js watch",
		"kitdev": "export NODE_OPTIONS=--max_old_space_size=6144 && svelte-kit dev",
		"dev": "npm-run-all -p -r watch \"kitdev -- {@} \" --",
		"prebuild": "node urara.js build",
		"build": "export NODE_OPTIONS=--max_old_space_size=6144 && svelte-kit build",
		"postbuild": "node urara.js clean",
		"preview": "svelte-kit preview",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare-workers": "^1.0.0-next.23",
		"@sveltejs/adapter-static": "^1.0.0-next.19",
		"@sveltejs/kit": "next",
		"@sveltejs/svelte-virtual-list": "^3.0.1",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"@windicss/plugin-animations": "^1.0.9",
		"daisyui": "^1.14.1",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-svelte3": "^3.2.0",
		"mdsvex": "^0.9.8",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"rehype-external-links": "^1.0.0",
		"rehype-slug": "^5.0.0",
		"remark-abbr": "^1.4.1",
		"remark-github": "^11.1.0",
		"remark-spoiler": "^1.0.6",
		"shiki": "^0.9.6",
		"svelte": "^3.34.0",
		"svelte-check": "^2.0.0",
		"svelte-headroom": "^2.2.1",
		"svelte-preprocess": "^4.0.0",
		"theme-change": "^2.0.2",
		"tslib": "^2.0.0",
		"typescript": "^4.0.0",
		"vite-plugin-windicss": "^1.2.4"
	},
	"type": "module"
}
